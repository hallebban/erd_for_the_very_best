<div class="page-header">
  <h3>Best At #<%= @best_at.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @best_at.user_id %></dd>

      <dt>Dish</dt>
      <dd><%= @best_at.dish_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/best_ats" class="btn btn-primary">
        Back
      </a>
      <a href="/best_ats/<%= @best_at.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_best_at/<%= @best_at.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
